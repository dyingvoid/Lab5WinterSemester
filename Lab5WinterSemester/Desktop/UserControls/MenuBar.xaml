<UserControl x:Class="Lab5WinterSemester.Desktop.UserControls.MenuBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Lab5WinterSemester.Desktop.UserControls"
             xmlns:desktop="clr-namespace:Lab5WinterSemester.Desktop"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="grid">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Name="BtnDbOpen">Open DB</Button>
        <ToggleButton Grid.Row="1" Name="TogBtnExplorer">Explorer</ToggleButton>
        <ToggleButton Grid.Row="2" Name="TogBtnMetaData">MetaData</ToggleButton>
        <ToggleButton Grid.Row="3" Name="TogBtnData">Data</ToggleButton>
    </Grid>
</UserControl>
